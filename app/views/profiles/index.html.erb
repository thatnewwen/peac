<div class="page-header"><h1>Profiles</h1></div>

	<table id="table_id">
	  <thead>
		  <tr>
			  <th>ID</th>
		    <th>First Name</th>
		    <th>Last Name</th> 
		    <th>University</th>
		    <th>Graduation Year</th>
		    <th>Company</th>
		    <th>Location</th>
		    <th>Gender</th>
		    <th>Start Date</th>
		    <th>Resume</th>
		    <th>Delete</th>
		  </tr>
	  </thead>
	  <tbody>
			<%= render @profiles %>
		</tbody>
	</table>
	<%= link_to 'Download All', download_all_path , target: '_blank' %>
	<%= link_to 'Generate Selected', download_selected_path , target: '_blank', class: 'selected' %>
	<%= link_to 'Download Selected', combined_selected_path , target: '_blank' %>
