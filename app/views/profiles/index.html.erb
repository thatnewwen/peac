<div class="admin-container">
<div class="page-header"><h1>Profiles</h1></div>
	<div class="download-form">
	<div class="download-form-title">Download Selected</div>
	<a class="create-pdf" href="/">Create PDF based on table.</a>
	<a class="download-button" href="/combined_selected">Download PDF.</a>

	</div>
	<table id="table_id">
	  <thead>
		  <tr>
			  <th>ID</th>
		    <th>First Name</th>
		    <th>Last Name</th> 
		    <th>University</th>
		    <th>Graduation Year</th>
		    <th>Company</th>
		    <th>Current Occupation</th>
		    <th>Current Location</th>
		    <th>Current Region</th>
		    <th>Gender</th>
		    <th>Start Date with Current Firm</th>
		    <th>Preferred PE Start Date</th>
		    <th>Industry Preference (First)</th>
		    <th>Industry Preference (Second)</th>
		    <th>Industry Preference (Third)</th>
		    <th>Interested Types</th>
		    <th>Submitted At</th>
		    <th>Resume</th>
		    <th>Edit</th>
		    <th>Delete</th>
		  </tr>
	  </thead>
	  <tbody>
			<%= render @profiles %>
		</tbody>
	</table>
	
</div>