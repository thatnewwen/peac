<tr>
  <td><%= profile.first_name %></td>
  <td><%= profile.last_name %></td> 
  <td><%= profile.university %></td>
  <td><%= profile.graduation_year %></td>
  <td><%= profile.company %></td>
  <td><%= profile.location %></td>
  <td><%= profile.gender %></td>
  <td><%= profile.start_date %></td>
  <td><%= link_to 'Download', profile.resume.url, target: '_blank' %></td>
	<td><%= link_to 'Remove', profile_path(profile), class: 'btn btn-danger', method: :delete, data: {confirm: 'Are you sure?'} %></td>
</tr>